				    <div class="leftlist">
				    <input class="button" name="g" type="submit" value="<%= 'Adventure'%>">
				    </div>
				    <div class="leftlist">
				    <input class="button" name="g" type="submit" value="<%= 'Animation'%>">
				    </div>
				    <div class="leftlist">
				    <input class="button" name="g" type="submit" value="<%= 'Comedy'%>">
